  number = {4},
  journal = {The Journal of Trauma and Acute Care Surgery},
  author = {Obeid, Nadia M. and Azuh, Ogochukwu and Reddy, Subhash and Webb, Shawn and Reickert, Craig and Velanovich, Vic and Horst, H. Mathilda and Rubinfeld, Ilan},
  month = apr,
