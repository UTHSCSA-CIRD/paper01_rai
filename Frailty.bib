  keywords = {Female,Humans,Logistic Models,Odds Ratio,Aged,Aged; 80 and over,Male,Middle Aged,United States,Quality Improvement,Severity of Illness Index,Retrospective Studies,Frail Elderly,Postoperative Complications,Colectomy,Critical Care,Laparoscopy},
